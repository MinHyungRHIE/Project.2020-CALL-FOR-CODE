                        <script type="text/javascript">

                            var getComment = function (index, pageSize, totalPage) {

                                var html = '';
                                if (index < 0) index = 0;

                                for (var i = 0; i < 6; i++) {
                                    $($('#pagination li')[i]).removeClass('active')
                                }
                                //if(index!=0)
                                $($('#pagination li')[index + 1]).addClass('active')

                                var pageGroup = index * pageSize;
                                var cnt = 1;
                                try {

                                    for (var i = pageGroup; (i < pageGroup + pageSize && i < totalPage); i++) {

                                        var nonMaskingCount = 0;
                                        if (initData.data[i].name !== null)
                                            nonMaskingCount = Math.ceil((initData.data[i].name.length) / 2);
                                        var maskingMemId = '';

                                        if (initData.data[i].name !== null) {

                                            maskingMemId +=
                                                initData
                                                    .data[i]
                                                    .name
                                                    .substr(0, nonMaskingCount);

                                            for (var k = nonMaskingCount; k < initData.data[i].name.length; k++) {
                                                maskingMemId += '*';
                                            }
                                        }

                                        //console.log(maskingMemId);/ ?;
                                        var resultPhone = (initData.data[i].phone === null) ? '' : initData.data[i].phone.substr(0, 9) + '****'
                                        html += `<tr>
                                              <td>
                                                 <span class="custom-checkbox">
                                                    <input type="checkbox" id="checkbox1" name="options[]" value="1">
                                                    <label for="checkbox1"></label>
                                                 </span>
                                              </td>
                                                    <td>${maskingMemId}</td>
                                                    <td>${initData.data[i].in === undefined ? '미입장' : initData.data[i].in}</td>
                                                          <td id="${initData.data[i]._id}" class="userOutButton${cnt++}">${initData.data[i].out === undefined || initData.data[i].out === null ? '미퇴장' : initData.data[i].out}</td>
                                                    <td>${resultPhone}</td>
                            
                                           </tr>
                                                         `;



                                    }

                                } catch (e) {
                                    console.log(e)
                                    return;
                                }
                                $('#userTable').html(html);
                                setOutEvent();

                                return;
                            }
                        </script>
